<head>
  <meta charset="UTF-8" />
    <link rel="stylesheet" href="style.css">
</head>


  <body>
<header class="header">
  <div class="navbar">
  <%= render partial:"twigs/header" %>
  </div>
</header>

<div class="twigbox">
      <%= form_tag('/ongoings/new', method: :get) do %>
      <input type="submit" value="sent">
      <% end %>
    </div>


</div>

<div class="ongoingcontents">
<% @ongoings.each do |ongoing| %>
  <div class="ongoingbox">
    <div class="ongoingtitle">
    <%= ongoing.projectname %>
    <%= link_to "詳細", "/ongoings/#{ongoing.id}/", method: :get %>
    </div>

      <div class="ongoingimage">
        <%= image_tag(ongoing.image) %>
      </div>

    <div class="ongoingsammury">
      <%= ongoing.summary %>
    </div>
    <% end %>

  </div>
</div>




<%= form_for @ongoing do |f| %>
  <%= f.file_field :image %>
<% end %>